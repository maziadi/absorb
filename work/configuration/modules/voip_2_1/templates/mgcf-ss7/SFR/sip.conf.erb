[general]
context=default
allowoverlap=no	
realm=alphalink.fr
bindport=5060
bindaddr=<%= service_addr %>
srvlookup=yes
disallow=all
allow=alaw
allow=ulaw
dtmfmode=rfc2833
language=fr
defaultexpirey=300
maxexpirey=3600
useragent=Alphalink VOIP Server 2.1.0
t1min=<%= asterisk_t1min %>
timert1=<%= sip_timer_t1 %>

register => sangoma@<%= service_addr -%>:5062

