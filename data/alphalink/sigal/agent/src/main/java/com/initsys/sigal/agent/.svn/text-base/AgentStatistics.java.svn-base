package com.initsys.sigal.agent;

public interface AgentStatistics {

    @SuppressWarnings("unchecked")
    public abstract void addBillableDuration(Integer duration);

    @SuppressWarnings("unchecked")
    public abstract void addDuration(Integer duration);

    @SuppressWarnings("unchecked")
    public abstract void addTreatmentDuration(Integer duration);

    @SuppressWarnings("unchecked")
    public abstract void addInviteLatency(Integer latency);

    public abstract void incrementBusyCauseCount();

    public abstract void incrementExceptionCount();

    public abstract void incrementCallCount();

    public abstract void incrementCongestionCauseCount();

    public abstract void incrementEstablishedCallCount();

    public abstract void incrementUnallocatedCauseCount();

}
