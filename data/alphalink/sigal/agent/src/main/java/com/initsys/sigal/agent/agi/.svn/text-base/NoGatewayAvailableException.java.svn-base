package com.initsys.sigal.agent.agi;

/**
 * Thrown when no gateway is available while trying Failover
 */
public class NoGatewayAvailableException extends RuntimeException {

    /** Serial version UID */
    /* private static final long serialVersionUID = 7854951067763635494L; */

    public NoGatewayAvailableException(String message, Throwable cause) {
        super(message, cause);
    }

    public NoGatewayAvailableException(String message) {
        super(message);
    }

}
