configuration:
  ON_MESSAGE_DELAY : 0
  name        : prov_apn
  timeout     : 30
  loglevel    : debug
  logdestination : syslog

  stomp_urls   :
    stomp1:
      username  : "<%= amq_user %>"
      password  : "<%= amq_passwd %>"
      host      : <%= amq_ip %>
      port      : 61613
      ssl       : false

  subscribes    :
    "/queue/<%= amq_queue %>" :
      selector: node_name='<%= hostname %>'

  format        : application/json       # Default is text (xml => text/xml | json => application/json)

  daemon        :
    db_uri      : "postgres://<%= db_user %>:<%= db_passwd %>@<%= virtual_ip %>/<%= db_name %>"
    acct_pwd    : "password"
    ggsn_ip1    : <%= ggsn_ip1 %>
    ggsn_ip2    : <%= ggsn_ip2 %>

