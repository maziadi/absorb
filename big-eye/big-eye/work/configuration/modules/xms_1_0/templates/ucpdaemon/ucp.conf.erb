logs:
  mode: info
  file: /var/log/xms/ucp.log

database:
  username: <%= db_user %>
  password: <%= db_password %>
  dbname: <%= db_name %>
  host: <%= db_host %>

ucp:
  sc: <%= ucp_sc %>
  password: <%= ucp_password %>
  gateway: <%= ucp_gateway %>
  port: <%= ucp_port %>
  conn_attempt: <%= ucp_conn_attempt %>

