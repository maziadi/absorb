<cluster_property_set id="bootstrap">
        <attributes>
                <nvpair id="bootstrap01" name="transition-idle-timeout" value="60"/>
                <nvpair id="bootstrap02" name="default-resource-stickiness" value="INFINITY"/>
                <nvpair id="bootstrap03" name="default-resource-failure-stickiness" value="-500"/>
                <nvpair id="bootstrap04" name="symmetric-cluster" value="true"/>
                <nvpair id="bootstrap05" name="no-quorum-policy" value="stop"/>
                <nvpair id="bootstrap06" name="stop-orphan-resources" value="true"/>
                <nvpair id="bootstrap07" name="stop-orphan-actions" value="true"/>
                <nvpair id="bootstrap08" name="is-managed-default" value="true"/>
        </attributes>
</cluster_property_set>

