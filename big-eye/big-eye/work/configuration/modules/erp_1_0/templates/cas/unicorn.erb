worker_processes 2
working_directory "/srv/cas/"

# This loads the application in the master process before forking
# worker processes
# Read more about it here:
# http://unicorn.bogomips.org/Unicorn/Configurator.html
preload_app true

timeout 30

listen "<%= eval("ipaddress_#{iface_si}") %>:8097", :tcp_nopush => true

pid "/srv/cas/unicorn.pid"

stderr_path "/srv/cas/log/unicorn.stderr.log"
stdout_path "/srv/cas/log/unicorn.stdout.log"
