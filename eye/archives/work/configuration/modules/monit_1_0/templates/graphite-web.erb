check process nginx
  with pidfile "/var/run/nginx.pid"
  alert <%= monit_conf_alert_email %>

check process graphite-web
  with pidfile "/var/run/gunicorn/graphite-web.pid"
  start program = "/usr/sbin/gunicorn-debian start graphite-web"
  stop program = "/usr/sbin/gunicorn-debian stop graphite-web"
  alert <%= monit_conf_alert_email %>
