<?xml version="1.0" encoding="UTF-8"?>

<!-- ===================================================================== -->
<!-- This file contains the default descriptor for sip applications.       -->
<!-- ===================================================================== -->

<sip-app xmlns="http://www.jcp.org/xml/ns/sipservlet"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://www.jcp.org/xml/ns/sipservlet http://www.jcp.org/xml/ns/sipservlet/sip-app_1_1.xsd"
         version="1.1">
  <proxy-config>
    <proxy-timeout><%= routag_session_timeout %></proxy-timeout>
  </proxy-config>

  <session-config>
    <session-timeout><%= routag_session_timeout %></session-timeout>
  </session-config>
</sip-app>
