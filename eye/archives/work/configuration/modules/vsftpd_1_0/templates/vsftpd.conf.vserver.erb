<% if vsftpd_banner -%>
ftpd_banner=Welcome to alphalink FTP service
<% end -%>
anonymous_enable=NO
local_enable=YES
write_enable=YES
anon_upload_enable=YES
anon_other_write_enable=NO
chroot_local_user=YES
guest_enable=YES
guest_username=<%= vsftpd_guest_username %>
listen=YES
xferlog_enable=YES
local_umask=022
local_max_rate=50000000
anon_max_rate=50000000
