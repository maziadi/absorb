configuration:
  name        : eligibility_bg_daemon
  timeout     : 180
  loglevel        : info
  logdestination  : file
  stomp_urls   : 
    stomp1:
      username  : "<%= active_mq_user_login -%>"
      password  : "<%= active_mq_user_password -%>"
      host      : <%= active_mq_ip %>
      port      : 61613
      ssl       : false    
  subscribes:
    "/queue/eligibility_main":
      :selector: "job_priority IN('1', '2', '3')"
  format        : application/json       # Default is text (xml => text/xml | json => application/json)
