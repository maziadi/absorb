<%= environment %>:
  # Templates to load with the first application load
  first_load_templates:
    - main_menu_main
    - main_menu_item
    - main_menu_group
    - main_menu_section
    - path_item
    - sidebar_item
    - sidebar_box

  apps:
    base:
      url: 'https://<%= url %>'
      no_menu: true
    cas:
      url: 'https://<%= url %>/sso'
      no_menu: true
    uiXIV:
      no_menu: true
    hector:
      no_menu: true
    apnf:
      no_menu: true
    taxman:
      menu_path: "/api/trusted/ui/menu.json"
      prefix_api_aliases:
        - 'mobile_accounts'
        - 'usages'
        - 'offers'
        - 'cut_off'
        - 'exports'
    drive:
      menu_path: "/api/trusted/ui/menu.json"
      prefix_api_aliases:
        - 'products'
        - 'resources'
        - 'resource'
        - 'templates'
        - 'mvno_resources'
    gestion:
      docs: "http://<%= erp_ip %>:80"
      prefix_api_aliases:
        - 'orders'
        - 'order'
        - 'rest'
    anderson:
      prefix_api_aliases:
      delete_prefix_alias:
        - true
    pouss_mouss:
      prefix_api_aliases:
        - 'rio'
        - 'outbound'
    killbill:
      menu_path: "/api/trusted/ui/menu.json"
      prefix_api_aliases:

  time_out_backend: 90
  time_out_menu: 2
