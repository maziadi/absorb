<%= si_environment %>:
  vat:
    default_zone: 'france_met'
    default_value: 'vat_normal'
    zones:
      france_met:
        values:
          vat_normal: 19.6
      domtom:
        values:
          vat_normal: 8.5
      cee:
        values:
          vat_normal: 0
      other:
        values:
          vat_normal: 0
  apps:
    gestion:
      url: "http://<%= erp_ip %>:<%= erp_port %>"
      prefix: "/gestion"
      api_path:
        - "/rest"
        - "/api"

    anderson:
      url: "http://<%= anderson_ip %>:<%= anderson_port %>"
      prefix: "/erp-tech"
      api_path: 
        - "/api"
        - '/rest'
    taxman:
      url: "http://<%= taxman_ip %>:<%= taxman_port  %>/"
      prefix: "/voip"
      api_path:
        - "/rest"
        - "/api"
    hector:
      url: "http://<%= hector_ip %>:<%= hector_port %>"
      database:
        host: "<%= db_host %>"
        name: "<%= erp_db_name %>"
        username: "<%= hector_db_user %>"
        password: "<%= hector_db_passwd %>"
    pouss_mouss:
      url: "http://<%= pouss_mouss_ip %>:<%= pouss_mouss_port %>"
      prefix: "/pnm"
      api_path:
        - "/rest"
        - "/api"
    uiXIV:
      url: "http://<%= uixiv_ip %>:<%= uixiv_port %>"
      prefix: ""
      api_path: ""
    drive:
      url: "http://<%= drive_ip %>:<%= drive_port %>"
      prefix: "/drive"
      api_path:
        - "/rest"
        - "/api"
    apnf:
      url: "http://<%= apnf_ip %>:<%= apnf_port %>"
      prefix: ""
      api_path: "/api"
    killbill:
      url: "http://<%= killbill_ip %>:<%= killbill_port %>"
      prefix: "/killbill"
      api_path:
        - "/api"
  brokers:
    drive:
      connection:
        host: '<%= amq_si_host %>'
        port: '61613'
        user: '<%= amq_si_user %>'
        password: '<%= amq_si_passwd %>'
      queues:
        anderson: "anderson"
        gestion: "gestion"
        taxman: "taxman"
        killbill: "killbill"
        pouss_mouss: "pouss_mouss"
        drive: "drive"
    obao:
      connection:
        host: '<%= amq_erp_host %>'
        port: '61613'
        user: '<%= amq_erp_user %>'
        password: '<%= amq_erp_passwd %>'
      queues:
        main: "sfr_prov"
      nodes:
        obao: "obao"
        anderson: "anderson"
        pouss_mouss: "pouss_mouss"
    clockwork:
      connection:
        host: '<%= amq_erp_host %>'
        port: '61613'
        user: '<%= amq_erp_user %>'
        password: '<%= amq_erp_passwd %>'
      queues:
        download: "clockwork_download"
        upload: "clockwork_upload"

